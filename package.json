{
    "name": "workspacetheme",
    "displayName": "Workspace Theme",
    "description": "Set the theme for your current workspace",
    "version": "0.0.1",
    "publisher": "blueneogeo",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Themes"
    ],
    "activationEvents": [
        "onCommand:extension.setWorkspaceTheme"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.setWorkspaceTheme",
                "title": "Set Workspace Theme"
            }
        ],
        "keybindings": [
            {
                "key": "cmd+k cmd+shift+t",
                "command": "extension.setWorkspaceTheme"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
